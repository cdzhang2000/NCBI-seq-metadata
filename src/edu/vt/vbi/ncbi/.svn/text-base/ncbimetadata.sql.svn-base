create table sequencemeta.ncbimetadata(
DOMAIN varchar2(4000),
ORGANISM_NAME varchar2(4000),
STRAIN varchar2(4000),
SEROVAR_BIOVAR varchar2(4000),
CULTURE_COLLECTION varchar2(4000),
TYPE_STRAIN varchar2(4000),
PROJECT_STATUS varchar2(4000),
AVAILABILITY varchar2(4000),
SEQUENCING_CENTERS varchar2(4000),
COMPLETION_DATE varchar2(4000),
PUBLICATION varchar2(4000),
NCBI_TAXON_ID varchar2(4000),
NCBI_PROJECT_ID varchar2(4000),
GENBANK_ACCESSIONS varchar2(4000),
REFSEQ_ACCESSIONS varchar2(4000),
SEQUENCING_STATUS varchar2(4000),
CHROMOSOMES varchar2(4000),
PLASMIDS varchar2(4000),
CONTIGS varchar2(4000),
GENOME_SIZE varchar2(4000),
GC_CONTENT varchar2(4000),
ISOLATION_SITE varchar2(4000),
ISOLATION_SOURCE varchar2(4000),
ISOLATION_COMMENTS varchar2(4000),
COLLECTION_DATE varchar2(4000),
ISOLATION_COUNTRY varchar2(4000),
GEOGRAPHIC_LOCATION varchar2(4000),
LATITUDE varchar2(4000),
LONGITUDE varchar2(4000),
ALTITUDE varchar2(4000),
DEPTH varchar2(4000),
HOST_NAME varchar2(4000),
HOST_GENDER varchar2(4000),
HOST_AGE varchar2(4000),
HOST_HEALTH varchar2(4000),
GRAM_STAIN varchar2(4000),
CELL_SHAPE varchar2(4000),
MOTILITY varchar2(4000),
SPORULATION varchar2(4000),
TEMPERATURE_RANGE varchar2(4000),
OPTIMAL_TEMPERATURE varchar2(4000),
SALINITY varchar2(4000),
OXYGEN_REQUIREMENT varchar2(4000),
HABITAT varchar2(4000),
HOST varchar2(4000),
DISEASE varchar2(4000),
COMMENTS varchar2(4000)
)


update sequencemeta.ncbimetadata set comments=(select comments from sequencemeta.ncbicomment where pid=id)

update sequencemeta.ncbimetadata t1 set comments= (select null from sequencemeta.ncbicomment t2
where t2.comments='No description provided.' and pid=id )
